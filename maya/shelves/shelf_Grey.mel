        -command "//Control Offset Group : CoG\n//Selection: Parent, Curve (control)\nstring $con_selection[] = `ls -sl`;\n\nif (size($con_selection) < 1){\n    warning \"Selection must be the Joint and the Control Curve\";\n}\nelse{\n    undoInfo -ock;\n    //freeze transform of the control\n    makeIdentity -apply true -t 1 -r 1 -s 1 -n 0 -pn 1 $con_selection[1];\n    \n    //create offset Group\n    string $offsetGrp = `group -n ($con_selection[0] + \"_offset\") $con_selection[1]`;\n    \n    //match pivot\n    matchTransform -piv $offsetGrp $con_selection[1];\n    \n    // create Parent Constraint\n    string $prt_contrr[] = `parentConstraint $con_selection[0] $offsetGrp`;\n    \n    //delete Parent Constraint\n    delete $prt_contrr;\n    \n    //rename contorl\n    rename $con_selection[1] ($con_selection[0] + \"_ctrl\");\n    \n    undoInfo -cck;\n}\n" 
